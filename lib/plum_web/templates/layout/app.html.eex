  <body data-js-view-path="<%= @conn.assigns[:view_name] || js_view_path(@conn, @view_template) %>">
    <div class="d-flex justify-content-center">
      <%= if get_flash(@conn, :info) do %>
        <div class="alert alert-main alert-primary alert-dismissible" role="alert">
          <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <%= get_flash(@conn, :info) %>
        </div>
      <% end %>

      <%= if get_flash(@conn, :error) do %>
        <div class="alert alert-main alert-danger alert-dismissible" role="alert">
          <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
          <%= get_flash(@conn, :error) %>
        </div>
      <% end %>
    </div>
    <main role="main">
      <%= render @view_module, @view_template, assigns %>
    </main>
  </body>


  <%= if current_user = @conn.assigns |> Map.get(:current_user) do %>
