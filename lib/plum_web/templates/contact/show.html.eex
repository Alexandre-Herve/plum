<h2>Show Contact</h2>

<ul>

  <li>
    <strong>Annonce:</strong>
    <%= link @contact.ad.id, to: ad_path(@conn, :show, @contact.ad) %>
  </li>

  <li>
    <strong>Nom:</strong>
    <%= @contact.name %>
  </li>

  <li>
    <strong>Email:</strong>
    <%= @contact.email %>
  </li>

  <li>
    <strong>Phone:</strong>
    <%= @contact.phone %>
  </li>

</ul>

<span><%= link "Edit", to: contact_path(@conn, :edit, @contact) %></span>
<span><%= link "Back", to: contact_path(@conn, :index) %></span>
