<h2>Listing Lands</h2>

<table class="table">
  <thead>
    <tr>
      <th>Surface</th>
      <th>Lat</th>
      <th>Lng</th>
      <th>Price</th>
      <th>City</th>
      <th>Department</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for land <- @lands do %>
    <tr>
      <td><%= land.surface %></td>
      <td><%= land.lat %></td>
      <td><%= land.lng %></td>
      <td><%= land.price %></td>
      <td><%= land.city %></td>
      <td><%= land.department %></td>

      <td class="text-right">
        <span><%= link "Show", to: land_path(@conn, :show, land), class: "btn btn-default btn-xs" %></span>
        <span><%= link "Edit", to: land_path(@conn, :edit, land), class: "btn btn-default btn-xs" %></span>
        <span><%= link "Delete", to: land_path(@conn, :delete, land), method: :delete, data: [confirm: "Are you sure?"], class: "btn btn-danger btn-xs" %></span>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "New Land", to: land_path(@conn, :new) %></span>
